{
    "manifest_version": 3,
    "name": "Wakatime Tracker",
    "version": "1.0",
    "description": "Measure coding time and send information to Wakatime",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/*",
                "https://practice.geeksforgeeks.org/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "action": {
        "default_title": "Measure coding time and send information to Wakatime"
    } , "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}